-- DONT TOUCH DONT COPY IT WILL INSTA BAN
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")

local player = Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local function tweenColor(guiObject, startColor, endColor, duration)
    local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Linear, Enum.EasingDirection.InOut, -1, true)
    local goal = {}

    if guiObject:IsA("ImageLabel") then
        goal = {ImageColor3 = endColor}
    elseif guiObject:IsA("TextLabel") then
        goal = {TextColor3 = endColor}
    end

    local tween = TweenService:Create(guiObject, tweenInfo, goal)
    tween:Play()
end

local function updateBarAndTextColor()
    local screenGui = playerGui:FindFirstChild("ScreenGui")
    if not screenGui then return end

    local magicHealthFrame = screenGui:FindFirstChild("MagicHealth")
    if not magicHealthFrame then return end

    local healthFrame = magicHealthFrame:FindFirstChild("Health")
    if healthFrame then
        local barFrame = healthFrame:FindFirstChild("Bar")
        if barFrame then
            local imageLabel = barFrame:FindFirstChild("Bar")
            if imageLabel and imageLabel:IsA("ImageLabel") then
                imageLabel.ImageColor3 = Color3.fromRGB(200, 150, 255)
                tweenColor(imageLabel, Color3.fromRGB(200, 150, 255), Color3.fromRGB(128, 0, 128), 3)
            end
        end
    end

    local textLabel = magicHealthFrame:FindFirstChild("TextLabel")
    if textLabel and textLabel:IsA("TextLabel") then
        textLabel.Text = "MENACING FORM"
        textLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
        tweenColor(textLabel, Color3.fromRGB(255, 255, 0), Color3.fromRGB(255, 255, 255), 3)
    end
end

local function onCharacterAdded(character)
    updateBarAndTextColor()
end

local function onPlayerAdded()
    local character = player.Character or player.CharacterAdded:Wait()
    onCharacterAdded(character)
    player.CharacterAdded:Connect(onCharacterAdded)
end

Players.PlayerAdded:Connect(onPlayerAdded)
if player then
    onPlayerAdded()
end

-- DONT TOUCH DONT COPY IT WILL INSTA BAN
local Players = game:GetService("Players")
local player = Players.LocalPlayer
local speaker = player
local checkingCutscene = false

local function StopFreecam()
    if workspace.CurrentCamera then
        workspace.CurrentCamera:Destroy()
    end

    repeat wait() until speaker.Character

    local humanoid = speaker.Character:FindFirstChildWhichIsA('Humanoid')
    if humanoid then
        workspace.CurrentCamera = Instance.new("Camera", workspace)
        workspace.CurrentCamera.CameraSubject = humanoid
        workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
    end

    speaker.CameraMinZoomDistance = 0.5
    speaker.CameraMaxZoomDistance = 400
    speaker.CameraMode = Enum.CameraMode.Classic

    local head = speaker.Character:FindFirstChild("Head")
    if head then
        head.Anchored = false
    end
end

task.spawn(function()
    while true do
        wait(0.5) -- Adjust as needed
        if workspace.CurrentCamera.CameraType == Enum.CameraType.Scriptable and not checkingCutscene then
            checkingCutscene = true -- Prevents multiple executions
            StopFreecam()
            wait(1)
            checkingCutscene = false
        end
    end
end)

