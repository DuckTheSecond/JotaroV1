local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()

local stand
local animations = {}
local standAnimationId = "12460977270"
local targetAnimId = "16002681909"
local standVisible = false

local function makePartsTransparent(standClone)
    for _, part in ipairs(standClone:GetDescendants()) do
        if part:IsA("BasePart") then
            if part.Name == "LeftFoot" or part.Name == "RightFoot" or part.Name == "LeftHand" or part.Name == "RightHand" then
                part.Transparency = 1
            end
        end
    end
end

local function fadeOutStand(standClone)
    for i = 0, 1, 0.2 do
        for _, part in ipairs(standClone:GetDescendants()) do
            if part:IsA("BasePart") then
                part.Transparency = i
            end
        end
        wait(0.05)
    end
    standClone:Destroy()
end

local function createStand()
    stand = character:Clone()
    stand.Name = "Stand"
    stand.Parent = workspace
    stand.HumanoidRootPart.Anchored = false
    makePartsTransparent(stand)
end

character.Humanoid.AnimationPlayed:Connect(function(animTrack)
    local animationId = animTrack.Animation.AnimationId

    if animationId == "rbxassetid://" .. targetAnimId and not standVisible then
        createStand()
        standVisible = true

        local frontOffset = character.HumanoidRootPart.CFrame.LookVector * 3 -- Move the stand closer (3 studs in front)
        stand:SetPrimaryPartCFrame(character:GetPrimaryPartCFrame() + frontOffset)

        local standAnim = Instance.new("Animation")
        standAnim.AnimationId = "rbxassetid://" .. standAnimationId
        local standTrack = stand.Humanoid:LoadAnimation(standAnim)
        standTrack:Play()
        standTrack:AdjustSpeed(0.9) -- Play the animation twice as fast (increase speed)

        game:GetService("RunService").Heartbeat:Connect(function()
            if character and stand and standVisible then
                local frontOffset = character.HumanoidRootPart.CFrame.LookVector * 3 -- Keep the stand closer (3 studs)
                stand:SetPrimaryPartCFrame(character:GetPrimaryPartCFrame() + frontOffset)
            end
        end)
    end
end)

game:GetService("RunService").Heartbeat:Connect(function()
    for _, animTrack in pairs(character.Humanoid:GetPlayingAnimationTracks()) do
        if animTrack.Animation.AnimationId == "rbxassetid://" .. targetAnimId and not animTrack.IsPlaying and standVisible then
            fadeOutStand(stand)
            standVisible = false
        end
    end
end)

