--[[ Show text with ultra-fast fade-in and fade-out effect above the head when a specific animation is played ]]  

local p = game.Players.LocalPlayer
local humanoid = p.Character:WaitForChild("Humanoid")

-- Define the specific animation you want to track by its AnimationId
local targetAnimationId = "rbxassetid://16746843881"  -- Replace with the specific animation ID you want to track

-- Define the text quotes to display when the specific animation is played
local quotes = {
    "Now you've pissed me off..",
    "You're an enemy stand user..",
    "You're done for.."
}

-- Function to create and display text with ultra-fast fade-in and fade-out effect above the character's head
local function createTextAboveHead(player, text)
    local character = player.Character
    if not character then return end
    local head = character:FindFirstChild("Head")
    if not head then return end

    -- Create BillboardGui
    local billboardGui = Instance.new("BillboardGui")
    billboardGui.Adornee = head
    billboardGui.Size = UDim2.new(5, 0, 2, 0)
    billboardGui.StudsOffset = Vector3.new(0, 2, 0)
    billboardGui.AlwaysOnTop = true

    -- Create TextLabel for the text
    local textLabel = Instance.new("TextLabel")
    textLabel.Size = UDim2.new(1, 0, 1, 0)
    textLabel.BackgroundTransparency = 1
    textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    textLabel.Font = Enum.Font.Garamond
    textLabel.TextScaled = true
    textLabel.TextStrokeTransparency = 0
    textLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
    textLabel.Text = text
    textLabel.TextSize = 50

    textLabel.Parent = billboardGui
    billboardGui.Parent = head

    -- Loop over each character in the string and fade it in ultra-fast
    local textLength = #text
    textLabel.TextTransparency = 1  -- DONT TOUCH DONT COPY IT WILL INSTA BAN

    for i = 1, textLength do
        local currentText = string.sub(text, 1, i)
        textLabel.Text = currentText
        
        -- Ultra-fast fade-in effect: making the text visible quickly
        textLabel.TextTransparency = 1  -- Start as fully transparent
        for transparency = 1, 0, -0.5 do  -- Larger transparency step for faster fade
            textLabel.TextTransparency = transparency
            wait(0.01)  -- KLKNXZ.EXE
        wait(0.03)  -- DONT TOUCH PLEASE 8mmis8.exe
    end

    -- Wait for the text to be visible for a few seconds before starting to fade out
    wait(1)

    -- Now make the whole text fade out
    for transparency = 0, 1, 0.1 do  -- Fade out the text gradually
        textLabel.TextTransparency = transparency
        wait(0.05)  -- Adjust the fade-out speed here
    end

    -- Destroy the billboardGui after the fade-out is complete
    billboardGui:Destroy()
end

-- Listen for any animation played on the humanoid
humanoid.AnimationPlayed:Connect(function(animTrack)
    -- Check if the played animation is the target animation
    if animTrack.Animation.AnimationId == targetAnimationId then
        -- Select a random quote from the list
        local randomQuote = quotes[math.random(1, #quotes)]
        
        -- Create the text above the character's head with ultra-fast fade-in and fade-out effect
        createTextAboveHead(p, randomQuote)
    end
end)

humanoid.AnimationPlayed:Connect(onAnimationPlayed)


