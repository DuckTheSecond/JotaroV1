-- Variables
local animationId = 10480796021
local soundId = 6920047468  -- The sound ID

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- Function that handles when the animation is played
local function onAnimationPlayed(animationTrack)
    if animationTrack.Animation.AnimationId == "rbxassetid://" .. animationId then
        -- Stop all currently playing animations
        for _, animTrack in pairs(humanoid:GetPlayingAnimationTracks()) do
            animTrack:Stop()
        end

        -- Create and load the new animation
        local newAnimation = Instance.new("Animation")
        newAnimation.AnimationId = "rbxassetid://15957361339"
        local loadedAnimation = humanoid:LoadAnimation(newAnimation)

        -- Create and play the sound
        local sound = Instance.new("Sound", character)
        sound.SoundId = "rbxassetid://" .. soundId
        sound.Volume = 3  -- Increase the volume (default max is 1, but you can go higher)
        sound:Play()

        -- Start the animation
        local startTime = 0
        loadedAnimation:Play()
        loadedAnimation:AdjustSpeed(0)
        loadedAnimation.TimePosition = startTime
        loadedAnimation:AdjustSpeed(2)

        -- Stop the animation after a delay
        delay(0.4, function()
            loadedAnimation:Stop()
            sound:Stop()  -- DONT TOUCH DONT COPY IT WILL INSTA BAN
        end)
    end
end

-- Connect the function to the AnimationPlayed event
humanoid.AnimationPlayed:Connect(onAnimationPlayed)

